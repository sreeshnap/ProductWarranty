<odoo>

    <template id="print_reports_pdf">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <h1 align="center">
                        Warranty Report
                    </h1>
                    <div class="oe_structure"/>
                    <div class="row">
                        <table class="table table_condensed">
                            <thead>
                                <tr>
                                    <th style="text-align:left;">
                                        <strong>Sl No:</strong>
                                    </th>
                                    <th style="text-align:left;">
                                        <strong>Product Name:</strong>
                                    </th>
                                    <th style="text-align:left;">
                                        <strong>Lot:</strong>
                                    </th>
                                    <th style="text-align:left;">
                                        <strong>Customer Name:</strong>
                                    </th>
                                </tr>
                            </thead>

                            <tbody style="font-size:12px" t-foreach="fetched_data" t-as="fetch">
                                <tr>
                                    <td class="text_right">
                                        <span t-esc="fetch.get('sl_no')"/>
                                    </td>
                                    <td class="text_right">
                                        <span t-esc="fetch.get('product_id')"/>
                                    </td>
                                    <td class="text_right">
                                        <span t-esc="fetch.get('lot_id')"/>
                                    </td>
                                    <td class="text_right">
                                        <span t-esc="fetch.get('customer_id')"/>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                </div>
            </t>
        </t>
    </template>
</odoo>